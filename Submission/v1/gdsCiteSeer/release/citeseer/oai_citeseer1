   <dc:identifier>http://citeseer.ist.psu.edu/86.html</dc:identifier>
   <dc:source>http://cardit.et.tudelft.nl/~steven/ilp/mahlke92.ps.gz</dc:source>
   <dc:language>en</dc:language>
   <oai_citeseer:relation type="References">
      <oai_citeseer:uri>oai:CiteSeerPSU:93807</oai_citeseer:uri>
