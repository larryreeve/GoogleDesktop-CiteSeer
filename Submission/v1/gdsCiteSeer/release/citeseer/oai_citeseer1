   <dc:title>Correction of a Memory Management Method for Lock-Free Data Structures</dc:title>
   <oai_citeseer:author name="Maged M. Michael">
      <address>Rochester , NY 14627-0226</address>
      <affiliation>Department of Computer Science; University of Rochester</affiliation>
   </oai_citeseer:author>
