   <dc:title>Using PVM 3.0 to Run Grand Challenge Applications on a Heterogeneous Network of Parallel Computers</dc:title>
   <oai_citeseer:author name="Al Geist">
      <affiliation>Oak Ridge National Laboratory</affiliation>
   </oai_citeseer:author>
   <oai_citeseer:author name="Jack Dongarra">
      <affiliation>Oak Ridge National Laboratory and University of Tennessee</affiliation>
   </oai_citeseer:author>
   <oai_citeseer:author name="Robert Manchek">
      <affiliation>University of Tennessee</affiliation>
