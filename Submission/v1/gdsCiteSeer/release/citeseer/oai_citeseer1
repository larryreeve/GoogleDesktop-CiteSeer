   <dc:identifier>http://citeseer.ist.psu.edu/62.html</dc:identifier>
   <dc:source>http://cuiwww.unige.ch/~queloz/papers/mac3.1999.ps.gz</dc:source>
   <dc:language>en</dc:language>
   <oai_citeseer:relation type="References">
      <oai_citeseer:uri>oai:CiteSeerPSU:47221</oai_citeseer:uri>
   </oai_citeseer:relation>
   <oai_citeseer:relation type="References">
      <oai_citeseer:uri>oai:CiteSeerPSU:239733</oai_citeseer:uri>
   </oai_citeseer:relation>
   <oai_citeseer:relation type="Is Referenced By">
      <oai_citeseer:uri>oai:CiteSeerPSU:6</oai_citeseer:uri>
   </oai_citeseer:relation>
   <oai_citeseer:relation type="Is Referenced By">
