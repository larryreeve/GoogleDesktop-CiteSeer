<oai_citeseer:oai_citeseer xmlns:oai_citeseer="http://copper.ist.psu.edu/oai/oai_citeseer/" xmlns:dc ="http://purl.org/dc/elements/1.1/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://copper.ist.psu.edu/oai/oai_citeseer/ http://copper.ist.psu.edu/oai/oai_citeseer.xsd ">
   <dc:title>CSDC - The MoTiV Car Speech Data Collection</dc:title>
   <oai_citeseer:author name="Detlev Langmann">
      <address>Philips Innovation Center , Taipei , Taiwan , R.O.C.</address>
      <affiliation>Ute Jekosch</affiliation>
   </oai_citeseer:author>
   <oai_citeseer:author name="Hartmut R. Pfitzinger">
      <address>Philips Research Laboratories Aachen , Germany</address>
   </oai_citeseer:author>
