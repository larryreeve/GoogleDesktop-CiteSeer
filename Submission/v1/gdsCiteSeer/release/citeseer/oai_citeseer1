<oai_citeseer:oai_citeseer xmlns:oai_citeseer="http://copper.ist.psu.edu/oai/oai_citeseer/" xmlns:dc ="http://purl.org/dc/elements/1.1/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://copper.ist.psu.edu/oai/oai_citeseer/ http://copper.ist.psu.edu/oai/oai_citeseer.xsd ">
   <dc:title>Working Memory and Dyslexia</dc:title>
   <oai_citeseer:author name="Alan D. Baddeley">
      <affiliation>MRC Applied Psychology Unit; University of Sheffield Cambridge</affiliation>
   </oai_citeseer:author>
   <oai_citeseer:author name="Angela J. Fawcett">
      <affiliation>MRC Applied Psychology Unit; University of Sheffield Cambridge</affiliation>
   </oai_citeseer:author>
   <oai_citeseer:author name="Roderick I. Nicolson">
