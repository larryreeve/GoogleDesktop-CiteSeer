   <dc:publisher>unknown</dc:publisher>
   <dc:date>1998-04-20</dc:date>
   <dc:format>ps</dc:format>
   <dc:identifier>http://citeseer.ist.psu.edu/24.html</dc:identifier>
   <dc:source>http://ca.informatik.uni-oldenburg.de/publications/brockmeyer98a.ps</dc:source>
   <dc:language>en</dc:language>
   <oai_citeseer:relation type="Is Referenced By">
      <oai_citeseer:uri>oai:CiteSeerPSU:5</oai_citeseer:uri>
