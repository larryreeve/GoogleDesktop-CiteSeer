   <dc:format>ps</dc:format>
   <dc:identifier>http://citeseer.ist.psu.edu/6.html</dc:identifier>
   <dc:source>http://www.ecole-navale.fr/www.gts/collet/articleICIP96.ps</dc:source>
   <dc:language>en</dc:language>
