   <dc:identifier>http://citeseer.ist.psu.edu/72.html</dc:identifier>
   <dc:source>http://www5.informatik.uni-erlangen.de/TeX/Literatur/ps-dir/1994/Niemann94:PDCa.ps.gz</dc:source>
   <dc:language>en</dc:language>
   <oai_citeseer:relation type="References">
      <oai_citeseer:uri>oai:CiteSeerPSU:49714</oai_citeseer:uri>
   </oai_citeseer:relation>
   <oai_citeseer:relation type="References">
      <oai_citeseer:uri>oai:CiteSeerPSU:22491</oai_citeseer:uri>
   </oai_citeseer:relation>
   <oai_citeseer:relation type="References">
      <oai_citeseer:uri>oai:CiteSeerPSU:340970</oai_citeseer:uri>
   </oai_citeseer:relation>
   <oai_citeseer:relation type="References">
      <oai_citeseer:uri>oai:CiteSeerPSU:213205</oai_citeseer:uri>
